@article{subelj_group_2014,
	title = {Group detection in complex networks: {An} algorithm and comparison of the state of the art},
	volume = {397},
	issn = {03784371},
	shorttitle = {Group detection in complex networks},
	url = {http://arxiv.org/abs/1305.5136},
	doi = {10.1016/j.physa.2013.12.003},
	abstract = {Complex real-world networks commonly reveal characteristic groups of nodes like communities and modules. These are of value in various applications, especially in the case of large social and information networks. However, while numerous community detection techniques have been presented in the literature, approaches for other groups of nodes are relatively rare and often limited in some way. We present a simple propagation-based algorithm for general group detection that requires no a priori knowledge and has near ideal complexity. The main novelty here is that different types of groups are revealed through an adequate hierarchical group refinement procedure. The proposed algorithm is validated on various synthetic and real-world networks, and rigorously compared against twelve other state-of-the-art approaches on group detection, hierarchy discovery and link prediction tasks. The algorithm is comparable to the state of the art in community detection, while superior in general group detection and link prediction. Based on the comparison, we also dis- cuss some prominent directions for future work on group detection in complex networks.},
	urldate = {2018-12-06},
	journal = {Physica A: Statistical Mechanics and its Applications},
	author = {Šubelj, Lovro and Bajec, Marko},
	month = mar,
	year = {2014},
	note = {arXiv: 1305.5136},
	keywords = {Computer Science - Social and Information Networks, Physics - Data Analysis, Statistics and Probability, Physics - Physics and Society},
	pages = {144--156},
	annote = {Comment: 15 pages, 6 figures, 6 tables}
}

