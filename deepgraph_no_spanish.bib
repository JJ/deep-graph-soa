@inproceedings{arenas_framework_nodate,
	title = {A {Framework} for {Distributed} {Evolutionary} {Algorithms}},
	url = {http://www.springerlink.com/link.asp?id=h4n29kbl69jvab4c},
	abstract = {his paper describes the recently released DREAM (Distributed Resource Evolutionary Algorithm Machine) framework for the automatic distribution of evolutionary algorithm (EA) processing through a virtual machine built from large numbers of individual machines linked by standard Internet protocols. The framework allows five different user entry points which depend on the knowledge and requirements of the user. At the highest level, users may specify and run distributed EAs simply by manipulating graphical displays. At the lowest level the framework turns becomes a P2P (Peer to Peer) mobile agent system, that may be used for the automatic distribution of a class of processes including, but not limited to, EAs.},
